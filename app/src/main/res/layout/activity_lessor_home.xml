<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".LessorHomeActivity"
    android:id="@+id/layout_Relative1">
    <include layout="@layout/layout_simple_toolbar"/>

    <TextView
        android:id="@+id/textWeclomeLessor"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textAlignment="center"
        android:textSize="32sp"
        android:text="@string/welcome_lessor_home"
        android:layout_below="@+id/toolbar_simple"/>

    <TextView
        android:id="@+id/textView5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textAlignment="center"
        android:textSize="24sp"
        android:textStyle="italic"
        android:text="@string/list_of_your_properties"
        android:layout_below="@+id/textWeclomeLessor"/>
    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/btnCreateProp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_gravity="end|bottom"
        android:layout_margin="16dp"
        android:background="@android:color/transparent"
        android:backgroundTint="@color/colorPrimary"
        android:src="@drawable/plus"
        app:rippleColor="@color/colorWhite" />
    <ListView
        android:id="@+id/list_property"
        android:padding="20dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView5">
    </ListView>



</RelativeLayout>